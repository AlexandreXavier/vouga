        e.preventDefault();          
        $(location).attr('href',$(this).attr('href'));           
    });
        
        $('.dropdown-menu.loggin a[data-toggle="tab"]').click(function (e) {
            e.preventDefault()    
            e.stopPropagation();
                
            //$(this).tab('show');
            
            var attr = $(this).attr('href')
            console.log(attr);
            $(this).parent().parent().find('.active').removeClass('active');
            $(this).parent().addClass('active');
        
            $('.btn-group .tab-content').find('.active').removeClass('active');
            $(attr).addClass('active');
        
        
        })
         $('.dropdown-menu.loggin .tab-content #login').click(function (e) {
            e.preventDefault()    
            e.stopPropagation();
        })
           $('.dropdown-menu.loggin .tab-content #password').click(function (e) {
            e.preventDefault()    
            e.stopPropagation();
        })
        
        /* $('.dropdown-menu.loggin .tab-content a').click(function (e) {
           
