            if (href) {
                 window.location = href;
            }
        
        })*/
        
        
        
        var routeForm = '/default/federaciones/clubfederacion-login';
        //console.log(routeForm);
	var order = '';
	
	$('#columnClubFed').submit(function(e) {
		var rel = $(this).attr('rel');
		
		if (rel == 'noajax') {
			$(this).attr('rel','');
			$(this).attr('action',routeForm);
			return true;
		}
		else {
			var request = new FormData();    
		
			request.append('login', $('#columnClubFed #login').val());
			request.append('password', $('#columnClubFed #password').val());
			
			
			// Enviamos el formulario usando AJAX
			$.ajax({
				type: 'POST',
				url: routeForm,
				data: request,
				processData : false,
    			contentType : false,
				success: function(data) {
					if (data.error == 0) {
						//$('#columnClubFed').parent().html(data);
						//window.open(data);
						//$.fancybox.close();
                        console.log('');
						//document.location = '';
						if(data.location == 'federaciones')        
						       document.location = '/default/federaciones/start-account';
                        if(data.location == 'clubes')        
						       document.location = '/default/clubes/start-club-account';						               
						        
					}else{
                        $('#columnClubFed #formaccounterrortxt').show();
					    $('#columnClubFed #formaccounterrortxt .form-element.desc').text(data['formAccountERRORTXT']);	        
						 $('#loggin').addClass('open');        
                    }
				}
			});
			return false;
		}
	}); 

	$('#columnClubFed input[type="submit"]').click(function(e) {
		order = '&'+e.target.id+'=1';
	});
	$('#columnClubFed #ok').click(function(e) {
		e.preventDefault();
		$('#columnClubFed').submit();
	});
    //]]>
</script>
